 public class SellingCycle {
    public static void main(String[] args) {
        int oldCycle = Integer.parseInt(args[0]);
        int repair = Integer.parseInt(args[1]);
        int coloring = Integer.parseInt(args[2]);
        int Accesories = Integer.parseInt(args[3]);
        int profit = 1500;
        int sellingPrice=0;
        int totalPrice = (oldCycle + repair + coloring + Accesories);

        System.out.println("Selling Price:"+(totalPrice+profit));
    }
}
